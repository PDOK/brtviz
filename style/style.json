			"minzoom": 5,
			"line-cap": "butt",
			"line-join": "miter",
			"line-miter-limit": 2, 
			"filter": ["==", "type_weg", "autosnelweg"], 
			"paint": {
				"line-opacity": 1,
				"line-color": "#FCEF84",
				"line-width": 2
			}
		},
		{
			"id": "Wegdeellijn_mid",
			"type": "line",
			"source": "brt_achtergrond_source",
			"source-layer" : "wegdeellijn",
			"maxzoom": 20,
			"minzoom": 6,
			"line-cap": "butt",
			"line-join": "miter",
			"line-miter-limit": 2, 
			"filter": ["in", "type_weg", "hoofdweg", "regionale weg" ], 
			"paint": {
				"line-opacity": 1,
				"line-color": "#FBEE85",
				"line-width": 2
			}
		},
		{
			"id": "Wegdeellijn_lokaal",
			"type": "line",
			"source": "brt_achtergrond_source",
			"source-layer" : "wegdeellijn",
			"maxzoom": 20,
			"minzoom": 6,
			"line-cap": "butt",
			"line-join": "miter",
			"line-miter-limit": 2, 
			"filter": ["in", "type_weg","lokale weg"], 
			"paint": {
				"line-opacity": 1,
				"line-color": "#FEFEFE",
				"line-width": 2
			}
		},
		{
			"id": "Wegdeellijn_klein_achter",
			"type": "line",
			"source": "brt_achtergrond_source",
			"source-layer" : "wegdeellijn",
			"maxzoom": 20,
			"minzoom": 14,
			"line-cap": "butt",
			"line-join": "miter",
			"line-miter-limit": 2, 
			"filter": ["in", "type_weg", "straat", "overig", "veerverbinding"], 
			"paint": {
				"line-opacity": 1,
				"line-color": "#FEFEFE",
				"line-width": 1.5
			}
		},
				{
			"id": "Wegdeellijn_klein_voor",
			"type": "line",
			"source": "brt_achtergrond_source",
			"source-layer" : "wegdeellijn",
			"maxzoom": 20,
			"minzoom": 14,
			"line-cap": "butt",
			"line-join": "miter",
			"line-miter-limit": 2, 
			"filter": ["in", "type_weg", "straat", "overig", "veerverbinding"], 
			"paint": {
				"line-opacity": 1,
				"line-color": "#FFFFFF",
				"line-width": 1
			}
		},
		{
			"id": "Gebouwvlak",
			"type": "fill",
			"source": "brt_achtergrond_source",
			"source-layer" : "gebouwvlak",
			"maxzoom": 20,
			"minzoom": 12,
			"paint": {
				"fill-opacity": 1,
				"fill-color": "#D1D1D1",
				"fill-outline-color": "#D1D1D1"
			}
		},
		{
			"id": "spoorbaanlijn_achter",
			"type": "line",
			"source": "brt_achtergrond_source",
			"source-layer" : "spoorbaanlijn",
			"maxzoom": 20,
			"minzoom": 12,
			"line-cap": "round",
			"line-join": "miter",
			"line-miter-limit": 2, 
			"paint": {
				"line-opacity": 1,
				"line-color": "#A4A4A4",
				"line-width": 3
			}
		},
		{
			"id": "spoorbaanlijn_voor",
			"type": "line",
			"source": "brt_achtergrond_source",
			"source-layer" : "spoorbaanlijn",
			"maxzoom": 20,
			"minzoom": 12,
			"line-cap": "round",
			"line-join": "miter",
			"line-miter-limit": 2, 
			"paint": {
				"line-opacity": 1,
				"line-color": "#FFFFFF",
				"line-width": 1.5,
				"line-dasharray": [10.0, 10.0]
			}
		},
		{
			"id": "straatnamen",
			"type": "symbol",
			"source": "brt_achtergrond_source",
			"source-layer" : "wegdeellijn",
			"maxzoom": 20,
			"minzoom": 6,
			"line-cap": "butt",
			"line-join": "miter",
			"line-miter-limit": 2, 
			"symbol-placement": "line",
			"symbol-spacing" : 2,
			"symbol-avoid-edges" : true,
			"text-field":"nederlandse_straatnaam",
			"text-font": "Open Sans Regular",
			"text-size":12,
			"text-max-widht": 10,
			"text-line-height": 1.5,
			"paint": {
				"text-opacity": 1,
				"text-color": "rgb(0,0,0)",
				"text-halo-color": "rgb(0,0,0)",
				"text-halo-width": 2,
				"text-halo-blur":2
			}
		}
	]
}

