			"minzoom": 5,
			"line-cap": "butt",
			"line-join": "miter",
			"line-miter-limit": 2, 
			"filter": ["==", "type_weg", "autosnelweg"], 
			"paint": {
				"line-opacity": 1,
				"line-color": "#E69800",
				"line-width": 4
			}
		},
		{
			"id": "wegdeellijn_snelweg_voor",
