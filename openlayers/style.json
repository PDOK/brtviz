						[{"zoom":8, "value":"overig"		}, 1],
						[{"zoom":8, "value":"autosnelweg"	}, 5],
						[{"zoom":8, "value":"lokale weg"	}, 3],
						[{"zoom":8, "value":"regionale weg"	}, 3],
						[{"zoom":8, "value":"veerverbinding"}, 3]
					]
				}
			}
