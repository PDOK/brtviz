
{
	"version": 8,
	"name": "brt_a",
	"glyphs": "/font/{fontstack}/{range}.pbf",
	"sources": {
		"brt_achtergrond_source": {
			"type": "vector",
			"scheme": "tile",
			"tiles": [
				"http://localhost/brt_achtergrond/{z}/{x}/{y}.pbf"
			]
		}
	},	
	"layers":[
		// Background color
		{
			"id":  "background",
			"type": "background",
			"paint": {
				"background-color":"#333333"
			}
		},
		// 1. Layer Registratiefgebiedvlak POLYGON
		{
			"id": "Registratiefgebiedvlak",
			"type": "fill",
			"source": "brt_achtergrond_source",
			"source-layer" : "registratiefgebiedvlak",
			"maxzoom": 20,
			"minzoom": 0,
			"paint": {
				"fill-opacity": 1,
				"fill-color": {
					"property": "viscode",
					"stops":
					[
						[{"zoom":8, "value":"18081"}, "#80BDE3"],
						[{"zoom":8, "value":"18080"}, "#D6F1E9"]
					]
				},
				"fill-outline-color": "#80BDE3"
			}
		},
		// 2. Layer Terreinvlak POLYGON
		{
			"id": "Terreinvlak",
			"type": "fill",
			"source": "brt_achtergrond_source",
			"source-layer" : "terreinvlak",
			"maxzoom": 20,
			"minzoom": 0,
			"paint": {
				"fill-opacity": 1,
				"fill-color": {
					"property": "type_landgebruik",
					"stops":
					[
						[{"zoom":8, "value":"bos: grind"		}, "#80BDE3"],
						[{"zoom":8, "value":"bos: gemengd bos"	}, "#D6F1E9"],
						[{"zoom":8, "value":"zand"				}, "#80BDE3"],
						[{"zoom":8, "value":"bos: loofbos"		}, "#D6F1E9"],
						[{"zoom":8, "value":"heide"				}, "#80BDE3"],
						[{"zoom":8, "value":"bos: naaldbos"		}, "#D6F1E9"],
						[{"zoom":8, "value":"basaltblokken, steenglooiing"}, "#D6F1E9"]
					]
				},
				"fill-outline-color": "rgb(0,0,0)"
			}
		},
		// 3. Layer Waterdeelvlak POLYGON
		{
			"id": "Waterdeelvlak",
			"type": "fill",
			"source": "brt_achtergrond_source",
			"source-layer" : "waterdeelvlak",
			"maxzoom": 20,
			"minzoom": 0,
			"paint": {
				"fill-opacity": 1,
				"fill-color": {
					"property": "type_water",
					"stops":
					[ //Wss niet nodig
						[{"zoom":8, "value":"meer, plas"	}, "#80BDE3"],
						[{"zoom":8, "value":"waterloop"		}, "#D6F1E9"],
						[{"zoom":8, "value":"overig"		}, "#80BDE3"],
						[{"zoom":8, "value":"droogvallend"	}, "#D6F1E9"],
						[{"zoom":8, "value":"zee"			}, "#80BDE3"]
					]
				},
				"fill-outline-color": "rgb(0,0,0)"
			}
		},
		// 4. Layer Waterdeellijn LINE
		{
			"id": "Waterdeellijn",
			"type": "line",
			"source": "brt_achtergrond_source",
			"source-layer" : "waterdeellijn",
			"maxzoom": 20,
			"minzoom": 6,
			"line-cap": "butt",//default
			"line-join": "miter",//default
			"line-miter-limit": 2, //default
			"paint": {
				"line-opacity": 1,
				"line-color": "hsl(39, 41%, 86%)",
				"line-width": 1
			}
		},
		// 5. Layer Registratiefgebiedlijn LINE
		{
			"id": "Registratiefgebiedlijn",
